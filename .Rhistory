col_types = c("numeric", "blank", "numeric",
"numeric", "numeric", "blank"))
View(cp_pa_h_t_cc)
pca1 <- dudi.pca(cp_pa_h_t_cc, scannf = F, nf = 2)
round(pca1$eig/sum(pca1$eig)*100, 2)
plot(pca1$li[,1:2])
s.corcircle(pca1$co)
cor_current <- cor(cp_pa_h_t_cc, method="spearman")
cor_current[ upper.tri(cor_current, diag = T) ] <- NA
cor_current_resh <- na.omit( melt( cor_current) )
colnames(cor_current_resh) <- c("var1", "var2", "correlation")
library(ggplot2)
